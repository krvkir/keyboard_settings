    key <AD07> { overlay1 = <VU>, [ u, U, F7 ] };
    key <AD08> { overlay1 = <VI>, [ i, I, F8 ] };
    key <AD09> { overlay1 = <VO>, [ o, O, F9 ]};
    key <AD10> { overlay1 = <VQ>, [ p, P, F10 ] };
    key <AD11> { overlay1 = <VLBR>, [ bracketleft, braceleft, F11 ] };
    key <AD12> { overlay1 = <VRBR>, [ bracketright, braceright, F12 ] };

    key <AC01> { overlay1 = <VA>, [ a, A, 1 ] };
    key <AC02> { overlay1 = <VS>, [ s, S, 2 ] };
    key <AC03> { overlay1 = <VD>, [ d, D, 3 ] };
    key <AC04> { overlay1 = <VF>, [ f, F, 4 ] };
