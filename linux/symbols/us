    key <AC10> { overlay1 = <VSEM>, [ Control_R, Control_R, 0] };
    key <AC11> { overlay1 = <VAPO>, [ apostrophe, quotedbl, 0 ] };
    
    key <AB01> { overlay1 = <VZ>, [ z, Z, exclam ] };
    key <AB02> { overlay1 = <VX>, [ x, X, at ] };
    key <AB03> { overlay1 = <VC>, [ c, C, numbersign ] };
